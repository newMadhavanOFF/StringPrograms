public class CountVowels {
	public static void main(String[] args) {
		String s1="java";
		String s2=s1.toLowerCase();
		int count=0;
		for (int i = 0; i < s2.length(); i++) {
			char c1=s2.charAt(i);
			if (c1=='a'||c1=='e'||c1=='i'||c1=='o'||c1=='u') {
				count++;
				
			}
			System.out.println(count);
			
		}
	}
}
